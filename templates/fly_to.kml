<?xml version="1.0" encoding="UTF-8"?>
<kml xmlns="http://earth.google.com/kml/2.1">
    <Document>
        <LookAt >
            <longitude>{{ lon }}</longitude>
            <latitude>{{ lat }}</latitude>
            <altitude>{{ alt }}</altitude>
            <range>{{ range }}</range>
            <heading>45</heading>
            <tilt>30</tilt>
            <altitudeMode>absolute</altitudeMode> 
        </LookAt>
    </Document>
</kml>
